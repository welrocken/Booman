{
  "manifest_version": 2,

  "name": "Booman",
  "description": "This extension manages your bookmarks.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "background":{
    "scripts": ["common.js", "api.js", "string_extensions.js", "booman.js"]
  },

  "permissions": [
    "activeTab",
    "bookmarks"
  ]
}